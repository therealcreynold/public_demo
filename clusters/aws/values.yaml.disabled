      - libnftnl
      - policycoreutils-python-utils
      - cryptsetup
      - nfs-utils
      - iscsi-initiator-utils
      - zip
      - zstd
      ssh_authorized_keys:
      - ssh-rsa  clemenko@clemenko.local
