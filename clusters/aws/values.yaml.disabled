addons:
  monitoring:
    enabled: true
    values:
      global:
        cattle:
          systemDefaultRegistry: rgcrprod.azurecr.us
  longhorn:
    enabled: false
    values:
      global:
        cattle:
          systemDefaultRegistry: rgcrprod.azurecr.us
  neuvector:
    enabled: false
    values:
      global:
        cattle:
          url: https://rancher.ranchers.io
          systemDefaultRegistry: rgcrprod.azurecr.us
      controller:
        ranchersso:
          enabled: true
      k3s:
        enabled: true
      manager:
        svc:
          type: ClusterIP
      rbac: true