    requestSpotInstance: false
    tags: provisioner,rancher,KeepRunning,true
    retries: 5
    rootSize: 80
    sshUser: ec2-user
    volumeType: gp3
    vpcId: vpc-0b6941a3bdc1f3fda # required: replace with your vpc id
    useEbsOptimizedInstance: false
    usePrivateAddress: false
    userData: |
      #cloud-config
      packages:
      - iptables
